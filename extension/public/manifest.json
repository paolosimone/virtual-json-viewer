{
  "manifest_version": 2,
  "name": "JQ Json Viewer",
  "version": "0.1",
  "description": "",
  "permissions": ["<all_urls>", "webRequest", "webRequestBlocking"],
  "background": {
    "scripts": ["static/js/background.js"]
  },
  "content_scripts": [
    {
      "js": ["static/js/content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "192": "logo192.png"
  },
  "web_accessible_resources": ["jq.wasm.wasm"],
  "browser_action": {
    "default_icon": {
      "192": "logo192.png"
    },
    "default_title": "Popup",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html"
  }
}
